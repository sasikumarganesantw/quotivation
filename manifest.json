{

    "manifest_version": 2,
    "name": "Quotivation",
    "version": "1.0",
  
    "description": "Displays random inspiring quotes",
  
    "browser_action": {
      "default_icon": "icons/motiv.png"
    },
    "chrome_url_overrides" : {
      "newtab": "quotes.html"
    },
    "permissions": [
      "tabs"
    ],
  
    "background": [
      {
        "scripts": ["quotivation.js"],
        "persistent": false
      }
    ]
  }